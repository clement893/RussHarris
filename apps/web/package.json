{
  "name": "@modele/web",
  "version": "1.0.0",
  "description": "Next.js 16 frontend for MODELE-NEXTJS-FULLSTACK",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "validate:build": "node scripts/validate-build.js",
    "prebuild": "node scripts/ensure-css-file.js && pnpm api:manifest",
    "api:manifest": "node scripts/generate-frontend-api-manifest.js",
    "build": "node scripts/build-with-fallback.js",
    "build:turbopack": "USE_TURBOPACK=true node scripts/build-with-fallback.js",
    "build:webpack": "USE_WEBPACK=true node scripts/build-with-fallback.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit --incremental",
    "type-check:ci": "tsc --noEmit --incremental --pretty false",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf .next out .turbo",
    "env:validate": "node scripts/validate-env.js",
    "env:docs": "node scripts/validate-env.js docs",
    "env:generate": "node scripts/validate-env.js generate",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server ANALYZE=true next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser ANALYZE=true next build",
    "check:bundle-size": "node scripts/check-bundle-size.js",
    "audit:accessibility": "node scripts/audit-accessibility.js",
    "analyze:complexity": "node scripts/analyze-complexity.js",
    "test:load": "k6 run k6-load-test.js",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:e2e:all": "playwright test"
  },
  "dependencies": {
    "@axe-core/react": "^4.11.0",
    "@modele/types": "workspace:*",
    "@sentry/nextjs": "^10.32.1",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.1",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.1",
    "isomorphic-dompurify": "^2.9.0",
    "jose": "^6.1.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "next": "^16.1.0",
    "next-auth": "5.0.0-beta.20",
    "next-intl": "^4.6.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^2.12.7",
    "tailwindcss": "^3.4.1",
    "web-vitals": "^3.5.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.0",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-docs": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/nextjs": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/test": "^7.6.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.2.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^16.1.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
